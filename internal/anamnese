package anamnese

import (
	"fmt"
	"math"
)

func Anamnese () {
	nome := "Guilherme Pereira"
	idade := 21
	peso := 88.650
	altura := 1.77
	imc := peso / math.Pow(altura, 2)
	fmt.Printf("Ficha TÃ©cnica: \n")
	fmt.Printf("Nome.....: %v\n", nome)
	fmt.Printf("Idade....: %v\n", idade)
	fmt.Printf("Peso.....: %v\n", peso)
	fmt.Printf("Altura...: %v\n", altura)
	fmt.Printf("IMC......: %v", imc)
}
